	sudo ../../RAPL/main "/usr/local/jruby-9.1.7.0/bin/jruby -Xcompile.fastest=true -Xcompile.invokedynamic=true -J-server -J-Xmn512m -J-Xms2048m -J-Xmx2048m knucleotide.rb 0 < ../../knucleotide-input25000000.txt" JRuby k-nucleotide

run:
